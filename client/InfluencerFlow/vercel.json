{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ],
  "functions": {
    "api/contracts/preview.ts": {
      "maxDuration": 30
    },
    "api/contracts/generate.ts": {
      "maxDuration": 10
    },
    "api/contracts/sign.ts": {
      "maxDuration": 30
    },
    "api/payments/create-order.ts": {
      "maxDuration": 10
    },
    "api/payments/verify.ts": {
      "maxDuration": 10
    }
  }
}